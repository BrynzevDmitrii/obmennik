{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typesctript-eslint"
    ],
    "rules": {
      "semi":"off",
      "@typesctript-eslint/semi": [
        "warn"
      ],
      "@typesctript-eslint/no-empty-interface": [
        "error",
        {
          "allowSingleExtends": true
        }
      ]
    },
    "extends":[
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:@typesctript-eslint/eslint-recommended",
      "plugin:@typesctript-eslint/recommended"
  ]
  }
  